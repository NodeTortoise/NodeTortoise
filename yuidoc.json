{
    "name": "NodeTortoise",
    "description": "Proyecto Universitario para basado en archivos Galapagos independientes, ejecutado sobre una plataforma NodeJS",
    "version": "1.0.0",
    "url": "",
    "options": {
        "linkNatives": "true",
        "attributesEmit": "true",
        "selleck": "true",
        "exclude": "jquery,bootstrap,dataTables,metisMenu,sb-admin-2,public/apidocs,public/css,public/fonts,public/img,public/simulation",
        "ignorePaths": ["node_modules", "yuidocs", "bin"],
        "paths": ["*"],
        "outdir": "public/apidocs",
        "themedir": "yuidoc/theme/",
        "helpers": ["yuidoc/theme/helpers/helpers.js"]
    }
}