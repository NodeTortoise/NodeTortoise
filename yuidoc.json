{
    "name": "NodeTortoise",
    "description": "Proyecto Universitario para basado en archivos Galapagos independientes, ejecutado sobre una plataforma NodeJS",
    "version": "1.0.0",
    "url": "",
    "options": {
        "linkNatives": "true",
        "attributesEmit": "true",
        "selleck": "true",
        "ignorePaths": ["node_modules", "public", "yuidocs"],
        "paths": "server/*",
        "outdir": "public/apidocs",
        "themedir": "yuidoc/theme/",
        "helpers": ["yuidoc/theme/helpers/helpers.js"]
    }
}